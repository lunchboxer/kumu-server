{
  "name": "kumu-server",
  "scripts": {
    "start": "node src/index.js",
    "dev": "NODE_ENV='development' nodemon -e js,graphql -x node -r dotenv/config src/index.js",
    "debug": "nodemon -e js,graphql -x node --inspect -r dotenv/config src/index.js",
    "deploy": "export $(cat .env | xargs) && prisma deploy",
    "nodemon": "nodemon"
  },
  "dependencies": {
    "bcryptjs": "2.4.3",
    "compression": "^1.7.4",
    "graphql-binding": "^2.5.1",
    "graphql-shield": "^5.3.1",
    "graphql-yoga": "1.18.1",
    "jsonwebtoken": "8.5.1",
    "pinyin": "^2.8.3",
    "prisma-client-lib": "1.31.1",
    "serve-static": "^1.14.1",
    "sirv": "^0.4.2"
  },
  "devDependencies": {
    "dotenv": "6.2.0",
    "eslint": "^5.15.3",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-svelte3": "^2.7.3",
    "eslint-plugin-unicorn": "^10.0.0",
    "nodemon": "1.18.10",
    "prisma": "^1.34.3",
    "svelte": "^3.8.0"
  }
}
